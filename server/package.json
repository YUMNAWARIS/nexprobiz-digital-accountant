{
  "name": "nexprobiz-digital-accountant-server",
  "version": "1.0.0",
  "description": "Server for nexprobiz-digital-accountant-server",
  "main": "server.js",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "nodemon app.js",
    "create-migration": "knex migrate:make --knexfile ./knexfile.js",
    "migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "migrate-down": "knex migrate:rollback --knexfile ./knexfile.js",
    "create-seed": "knex seed:make --knexfile ./knexfile.js",
    "seed": "knex seed:run --knexfile ./knexfile.js",
    "seed-down": "knex seed:rollback --knexfile ./knexfile.js"
  },
  "keywords": [
    "nexprobiz",
    "accounting",
    "accountant"
  ],
  "author": "Yumna Waris",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "uuid": "^13.0.0",
    "yup": "^1.7.0"
  }
}
